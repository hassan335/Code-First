@model List<Khuari.Models.Customer>
@{
    ViewBag.Title = "Index";
}

<h1>Customers</h1>

<table class="table table-bordered table-hover">
    <tr>

        <th>Customer Name</th>
        <th>Membership Type</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.C_Name, "Edit", "Customers", new
           {
               id = item.C_Id
           }, null)
            </td>
            <td>
                @item.MembershipType.Name
            </td>
        </tr>
    }

</table>
